{
    "compilerOptions": {
        "target": "es2020",
        "sourceMap": true,
        "strict": true,
        "outDir": "./webroot/dist/",
        "noImplicitAny": true,
        "module": "es6",
        "jsx": "react",
        "allowJs": true,
        "moduleResolution": "node",
        "paths": {
            "@css/*": ["./src/css/*"],
            "@ts/*": ["./src/ts/*"],
            "@components/*": ["./src/components/*"],
        },
    }
}
